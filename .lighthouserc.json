{
  "ci": {
    "collect": {
      "url": ["http://localhost:8080/pomofocus-clone/"],
      "numberOfRuns": 3,
      "startServerCommand": "npm run preview",
      "settings": {
        "preset": "desktop",
        "onlyCategories": [
          "performance",
          "accessibility",
          "best-practices",
          "seo"
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
